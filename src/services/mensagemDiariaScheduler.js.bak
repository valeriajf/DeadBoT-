function getFaseLua(dataStr) {
  const hoje = new Date(dataStr);
  let faseAtual = "ðŸŒ‘ Nova";
  let menorDiff = Infinity;
  for (const entry of FASES_LUA) {
    const d = new Date(entry.data);
    const diff = hoje - d;
    if (diff >= 0 && diff < menorDiff) {
      menorDiff = diff;
      faseAtual = entry.fase;
    }
  }
  return faseAtual;
}

